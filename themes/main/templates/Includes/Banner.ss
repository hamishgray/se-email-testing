
<div id="banner" class="dark">
	<div class="banner-inner">
		<div class="v-align-inner">
			<div class="container">
				<div class="row">

					<% if BannerContent %>
						<div class="col-md-12">
							<h2>$BannerContent</h2>
						</div>
					<% else %>
						<div class="col-md-12">
							<h2>$Title</h2>
						</div>
					<% end_if %>


				</div>
			</div>
		</div>
	</div>
</div>
